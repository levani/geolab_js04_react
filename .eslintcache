sk', value: {id: id}});\n\n  useEffect(() => {\n    axios.get('https://us-central1-js04-b4877.cloudfunctions.net/tasks/')\n      .then(response => {\n        dispatch({type: 'load', value: response.data.data});\n      })\n      .catch(error => {\n        console.log(error);\n      })\n  }, []);\n\n  function onAddTask(id, text) {\n    let newTasks = [\n      {\n        id: id,\n        text: text,\n        completed: false\n      },\n      ...tasks\n    ];\n    // setTasks(newTasks);\n    dispatch({type: 'add_task', value: newTasks});\n  }\n\n  function onDelete(id) {\n    axios.delete('https://us-central1-js04-b4877.cloudfunctions.net/tasks/' + id)\n      .then(response => {\n        dispatch({type: 'delete_task', value: id});\n      })\n      .catch(error => {\n        console.log(error);\n      })\n  }\n\n  if (currentUser === null) {\n    return <Login />\n  }\n\n  return (\n    <>\n      <h2>Hello: {currentUser.name}</h2>\n      <AddTask onAddTask={onAddTask} />\n      {\n        tasks.map((task, i) => (\n          <Task\n            key={task.id}\n            text={task.text}\n            id={task.id}\n            completed={task.completed}\n            onDelete={onDelete}\n          />\n        ))\n      }\n    </>\n  )\n}","/Users/lm/Projects/geolab/my-app/src/ChangeTheme.js",[],"/Users/lm/Projects/geolab/my-app/src/UserContext.js",[],"/Users/lm/Projects/geolab/my-app/src/Login.js",["72"],"/Users/lm/Projects/geolab/my-app/src/i18n.js",[],"/Users/lm/Projects/geolab/my-app/src/CounterWithClass.js",[],{"ruleId":"73","replacedBy":"74"},{"ruleId":"75","replacedBy":"76"},{"ruleId":"77","severity":1,"message":"78","line":2,"column":17,"nodeType":"79","messageId":"80","endLine":2,"endColumn":23},{"ruleId":"77","severity":1,"message":"81","line":9,"column":8,"nodeType":"79","messageId":"80","endLine":9,"endColumn":15},{"ruleId":"77","severity":1,"message":"82","line":2,"column":8,"nodeType":"79","messageId":"80","endLine":2,"endColumn":14},{"ruleId":"77","severity":1