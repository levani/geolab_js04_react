rs/lm/Projects/geolab/my-app/src/Button.js",[],"/Users/lm/Projects/geolab/my-app/src/ThemeContext.js",[],"/Users/lm/Projects/geolab/my-app/src/Border.js",[],"/Users/lm/Projects/geolab/my-app/src/Tasks.js",["92","93"],"import AddTask from \"./AddTask\";\nimport Task from \"./Task\";\nimport axios from 'axios';\nimport { useContext, useEffect, useReducer, useState } from \"react\";\nimport Login from \"./Login\";\nimport UserContext from \"./UserContext\";\n\nconst initialState = [];\n\nfunction reducer(state, action) {\n  switch (action.type) {\n    case 'load':\n      return action.value;\n    case 'add_task':\n      return [...state, action.value];\n    case 'delete_task':\n      return state.filter(item => item.id !== action.value);\n    case 'complete_task':\n      return state.map(item => {});\n    default:\n      throw new Error();\n  }\n}\n\nexport default function Tasks() {\n  // const [tasks, setTasks] = useState([]);\n  const [tasks, dispatch] = useReducer(reducer, initialState);\n  const { currentUser } = useContext(UserContext);\n\n  // dispatch({type: 'complete_task', value: {id: id}});\n  // dispatch({type: 'uncheck_task', value: {id: id}});\n\n  useEffect(() => {\n    axios.get('https://us-central1-js04-b4877.cloudfunctions.net/tasks/')\n      .then(response => {\n        dispatch({type: 'load', value: response.data.data});\n      })\n      .catch(error => {\n        console.log(error);\n      })\n  }, []);\n\n  function onAddTask(id, text) {\n    let newTasks = [\n      {\n        id: id,\n        text: text,\n        completed: false\n      },\n      ...tasks\n    ];\n    // setTasks(newTasks);\n    dispatch({type: 'add_task', value: newTasks});\n  }\n\n  function onDelete(id) {\n    axios.delete('https://us-central1-js04-b4877.cloudfunctions.net/tasks/' + id)\n      .then(response => {\n        dispatch({type: 'delete_task', value: id});\n      })\n      .catch(error => {\n        console.log(error);\n      })\n  }\n\n  if (currentUser === null) {\n    return <Login />\n  }\n\n  return (\n    <>\n      <h2>Hello: {currentUser.name}</h2>\n      <AddTask onAddTask={onAddTask} />\n      {\n        tasks.map((task, i) => (\n          <Task\n            key={task.id}\n            text={task.text}\n            id={task.id}\n            completed={task.completed}\n            onDelete={onDelete}\n          />\n        ))\n      }\n    </>\n  )\n}","/Users/lm/Projects/geolab/my-app/src/ChangeTheme.js",[],"/Users/lm/Projects/geolab/my-app/src/UserContext.js",[],"/Users/lm/Projects/geolab/my-app/src/Login.js",["94"],"import { useContext, useState } from \"react\";\nimport { useTranslation } from \"react-i18next\";\nimport UserContext from \"./UserContext\";\n\nexport default function Login() {\n  const {currentUser, setCurrentUser} = useContext(UserContext);\n  const [username, setUsername] = useState('');\n  const [password, setPassword] = useState('');\n  const { t, i18n } = useTranslation();\n\n  function onSubmit() {\n    // axios.post('/login', {username, password})\n    //   .then(response => {\n    //     setCurrentUser({\n    //       username: response.user.username,\n    //       name: response.user.name,\n    //       email: response.user.email,\n    //     });\n    //   })\n    //   .catch(() => {\n    //     alert('Wrong username or password');\n    //   })\n\n    if (username === 'test' && password === 'qwerty') {\n      setCurrentUser({\n        username: 'test',\n        name: 'Giorgi'\n      });\n    } else {\n      alert('Wrong username or password');\n    }\n  }\n\n  function changeLanguage(lang) {\n    i18n.changeLanguage(lang);\n  }\n\n  return (\n    <form action=\"\" onSubmit={onSubmit}>\n      <p>\n        <input \n          type=\"text\" \n          name=\"username\"\n          placeholder={t('username')}\n          value={username}\n          onChange={e => setUsername(e.target.value)}\n        />\n      </p>\n      <p>\n        <input\n          type=\"password\"\n          name=\"password\"\n          placeholder={t('password')}\n          value={password}\n          onChange={e => setPassword(e.target.value)}\n        />\n      </p>\n      <p>\n        <button type=\"submit\">{t('login')}</button>\n      </p>\n      <p>\n        <button type=\"button\" onClick={() => changeLanguage('en')}>en</button>\n        <button type=\"button\" onClick={() => changeLanguage('ka')}>ka</button>\n      </p>\n    </form>\n  )\n}","/Users/lm/Projects/geolab/my-app/src/i18n.js",[],"/Users/lm/Projects/geolab/my-app/src/CounterWithClass.js",[],"/Users/lm/Projects/geolab/my-app/src/screens/Home.js",["95","96"],"/Users/lm/Projects/geolab/my-app/src/Header.js",["97"],"import { Link, useHistory } from \"react-router-dom\";\n\nexport default function Header() {\n  let history = useHistory();\n\n  return (\n    <nav>\n      <ul>\n        <li>\n          <Link to=\"/\">Home</Link>\n        </li>\n        <li>\n          <Link to=\"/blog\">Blog</Link>\n        </li>\n        <li>\n          <Link to=\"/about\">About</Link>\n        </li>\n        <li>\n          <Link to=\"/users\">Users</Link>\n        </li>\n      </ul>\n\n      {/* <button onClick={() => {\n        history.push(\"/users/123\");\n      }}>user 123</button> */}\n    </nav>\n  )\n}","/Users/lm/Projects/geolab/my-app/src/screens/Blog.js",["98","99","100","101","102","103","104","105","106","107","108","109"],"/Users/lm/Projects/geolab/my-app/src/screens/Post.js",["110","111","112"],{"ruleId":"113","replacedBy":"114"},{"ruleId":"115","replacedBy":"116"},{"ruleId":"117","severity":1,"message":"118","line":2,"column":17,"nodeType":"119","messageId":"120","endLine":2,"endColumn":23},{"ruleId":"117","severity":1,"message":"121","line":8,"column":3,"nodeType":"119","messageId":"120","endLine":8,"endColumn":9},{"ruleId":"117","severity":1,"message":"122","line":9,"column":3,"nodeType":"119","messageId":"120","endLine":9,"endColumn":8},{"ruleId":"117","severity":1,"message":"123","line":10,"column":3,"nodeType":"119","mes